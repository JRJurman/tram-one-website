{
  "name": "tram-one.github.io-v2",
  "version": "1.0.0",
  "scripts": {
    "lint": "xo",
    "start": "budo main.js:bundle.js --pushstate --dir public --live",
    "prebuild": "rm -rf dist && cp -r public/ dist",
    "build": "browserify ./main.js -o ./dist/bundle.js -t [ babelify --presets [ env ] ]",
    "postbuild": "tram-build-page -a ./main.js -r / -i ./public/index.html -o ./dist/index.html",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist -b master",
    "test": "jasmine ./specs/specs.js"
  },
  "keywords": [
    "tram-one"
  ],
  "dependencies": {
    "babel-preset-env": "^1.6.0",
    "babelify": "^7.3.0",
    "browserify": "^14.5.0",
    "budo": "^10.0.4",
    "eslint-config-tram-one": "^2.0.0",
    "gh-pages": "^1.1.0",
    "highlight-syntax": "^3.0.2",
    "jasmine": "^2.8.0",
    "tram-build-page": "^1.0.1",
    "tram-one": "^5.1.0",
    "xo": "^0.20.3"
  },
  "xo": {
    "extends": [
      "tram-one"
    ]
  }
}
